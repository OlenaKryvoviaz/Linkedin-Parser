{
  "name": "linkedin-pdf-downloader",
  "version": "1.0.0",
  "description": "Automated LinkedIn profile PDF downloader with email delivery",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test-session": "node test-session.js",
    "clear-session": "rm -f linkedin-cookies.json && echo 'Session cleared!'",
    "unblock": "node unblock-linkedin.js",
    "healthcheck": "node healthcheck.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d --build",
    "docker:prod:down": "docker-compose -f docker-compose.prod.yml down",
    "docker:prod:logs": "docker-compose -f docker-compose.prod.yml logs -f",
    "prod": "NODE_ENV=production node server.js",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop linkedin-pdf-downloader",
    "pm2:restart": "pm2 restart linkedin-pdf-downloader",
    "pm2:logs": "pm2 logs linkedin-pdf-downloader",
    "pm2:status": "pm2 status",
    "pm2:delete": "pm2 delete linkedin-pdf-downloader",
    "pm2:monit": "pm2 monit"
  },
  "keywords": [
    "linkedin",
    "pdf",
    "puppeteer",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "nodemailer": "^6.9.7",
    "puppeteer": "^24.35.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
